{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "javierdelpozolopez/club-dlp-theme",
                "version": "6.0",
                "type": "drupal-theme",
                "dist": {
                    "url": "https://github.com/javierdelpozolopez/club-dlp-theme/archive/6.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "billboardjs/billboard",
                "version": "1.6.2",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "billboard"
                },
                "dist": {
                    "url": "https://github.com/naver/billboard.js/archive/1.6.2.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "d3v4/d3",
                "version": "4.9.1",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "d3v4"
                },
                "dist": {
                    "url": "https://cdnjs.cloudflare.com/ajax/libs/d3/4.9.1/d3.js",
                    "type": "file"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "highcharts/highcharts",
                "version": "6.1.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "highcharts"
                },
                "dist": {
                    "url": "https://code.highcharts.com/6.1.0/highcharts.js",
                    "type": "file"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "highcharts/more",
                "version": "6.1.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "highcharts_more"
                },
                "dist": {
                    "url": "https://code.highcharts.com/6.1.0/highcharts-more.js",
                    "type": "file"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "highcharts/exporting",
                "version": "6.1.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "highcharts_exporting"
                },
                "dist": {
                    "url": "https://code.highcharts.com/6.1.0/modules/exporting.js",
                    "type": "file"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "highcharts/export-data",
                "version": "6.1.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "highcharts_export-data"
                },
                "dist": {
                    "url": "https://code.highcharts.com/6.1.0/modules/export-data.js",
                    "type": "file"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "highcharts/accessibility",
                "version": "6.1.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "highcharts_accessibility"
                },
                "dist": {
                    "url": "https://code.highcharts.com/6.1.0/modules/accessibility.js",
                    "type": "file"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "package",
                "package": {
                "name": "highcharts/3d",
                "version": "6.1.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "highcharts_3d"
                },
                "dist": {
                    "url": "https://code.highcharts.com/6.1.0/highcharts-3d.js",
                    "type": "file"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-gd": "*",
        "billboardjs/billboard": "1.6.2",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "d3v4/d3": "4.9.1",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^1.27",
        "drupal/auto_entitylabel": "^2.1@beta",
        "drupal/basic": "^2.0",
        "drupal/block_class": "^1.0",
        "drupal/blockgroup": "^1.1",
        "drupal/bulk_user_registration": "^1.1@alpha",
        "drupal/charts": "^3.0@beta",
        "drupal/claro": "^1.0@alpha",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.7.0",
        "drupal/devel": "^2.1",
        "drupal/feeds": "^3.0@alpha",
        "drupal/flysystem_s3": "^1.0@beta",
        "drupal/fontawesome": "^2.13",
        "drupal/fontawesome_menu_icons": "^1.5",
        "drupal/force_password_change": "^1.3",
        "drupal/mass_pwreset": "^1.0@alpha",
        "drupal/migrate_source_csv": "^2.2",
        "drupal/pathauto": "^1.4",
        "drupal/redirect": "^1.3",
        "drupal/redirect_after_login": "^2.5",
        "drupal/svg_image": "^1.9",
        "drupal/user_csv_import": "^1.4",
        "drupal/video": "^1.4",
        "drupal/views_aggregator": "1.x-dev",
        "drupal/views_bulk_operations": "^3.2",
        "drupal/views_cumulative_field": "^1.5",
        "drupal/views_fieldsets": "^3.3",
        "drupal/views_simple_math_field": "^1.9",
        "drush/drush": "^9.0.0",
        "javierdelpozolopez/club-dlp-theme": "^6.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.7.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
